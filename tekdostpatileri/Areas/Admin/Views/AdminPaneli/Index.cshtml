@using tekdostpatileri.Models;
@{
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}
@model ViewModel
@section başlık{
      TekDostPatileri
}
<link href="//maxcdn.bootstrapcdn.com/bootstrap/3.3.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
<link href="~/css/adminpanel.css" type="text/css" rel="stylesheet" />

<div class="container-fluid">
    <div class="row" id="main">
        <div class="col-sm-12 col-md-12 well" id="content">
            @if (TempData["HataM1"] != null)
            {
                <div class="alert alert-success" role="alert">
                    @TempData["HataM1"]
                </div>
            }
            @if (TempData["HataM2"] != null)
            {
                <div class="alert alert-danger" role="alert">
                    @TempData["HataM2"]
                </div>
            }
            @if (TempData["boş1"] != null)
            {
                <div>
                    <h2>Logo Resmi</h2>
                    <table class="table table-hover mx-auto text-center table-dark">
                        <thead class="text-white-50 text-opacity-100">
                            <tr>
                                <th scope="col">Id</th>
                                <th scope="col">Url</th>
                                <th scope="col">Sil</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model.logos)
                            {
                                <tr>
                                    <th scope="row">@item.Id</th>
                                    <th><img width="60" height="60" src="/icon/@item.LogoUrl" /></th>
                                    <th><a class="btn btn-danger btn-sm" href="~/Admin/AdminPaneli/Sil1/@item.Id">Sil</a></th>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            }
            @if (TempData["boş2"] != null)
            {
                <h2>Anasayfa İçerik</h2>
                    <div class="table-responsive">
                      
                        <table class="table table-hover mx-auto text-center table-dark">
                            @foreach (var anasayfa in Model.anasayfas)
                            {
                                <thead class="text-white-50 text-opacity-100">
                                    <tr>
                                        <th scope="col">Id</th>
                                        <th scope="col">Slider1Başlık</th>
                                        <th scope="col">Slider1Yazı</th>
                                        <th scope="col">Slider1İmg</th>
                                        <th scope="col">Slider2Başlık</th>
                                        <th scope="col">Slider2Yazı</th>
                                        <th scope="col">Slider2İmg</th>
                                        <th scope="col">Slider3Başlık</th>
                                        <th scope="col">Slider3Yazı</th>
                                        <th scope="col">Slider3İmg</th>
                                        <th scope="col">Slider4Başlık</th>
                                        <th scope="col">Slider4Yazı</th>
                                        <th scope="col">Slider4İmg</th>
                                        <th scope="col">Slider5Başlık</th>
                                        <th scope="col">Slider5Yazı</th>
                                        <th scope="col">Slider5İmg</th>
                                        <th scope="col">ÖnsözSesleniş</th>
                                        <th scope="col">Önsöz</th>
                                        <th scope="col">Sil</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>@anasayfa.Id</td>
                                        <td>@Html.Raw(anasayfa.slider1başlık)</td>
                                        <td>@Html.Raw(anasayfa.slider1yazı)</td>
                                        <td><img width="60" height="60" src="/img/Slider/@anasayfa.slider1img"/></td>
                                        <td>@Html.Raw(anasayfa.slider2başlık)</td>
                                        <td>@Html.Raw(anasayfa.slider2yazı)</td>
                                        <td><img width="60" height="60" src="/img/Slider/@anasayfa.slider2img"/></td>
                                        <td>@Html.Raw(anasayfa.slider3başlık)</td>
                                        <td>@Html.Raw(anasayfa.slider3yazı)</td>
                                        <td><img width="60" height="60" src="/img/Slider/@anasayfa.slider3img"/></td>
                                        <td>@Html.Raw(anasayfa.slider4başlık)</td>
