                                    <td><img width="60" height="60" src="/img/@item.kurucuresmi" /></td>
                                    <td>@Html.Raw(item.yazı1)</td>
                                    <td>@Html.Raw(item.yazı2)</td>
                                    <td>@Html.Raw(item.yazı3)</td>
